{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "<IMAGE_NAME>:<TAG>",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "3001"
    }
  ],
  "Logging": "/var/log/radar-api",
  "environment": [
    {
      "name": "NODE_ENV",
      "value": "dev"
    }
    {
      "name": "API_PORT",
      "value": "3000"
    }
    {
      "name": "CONSOLE_LOGLEVEL",
      "value": "debug"
    }
    {
      "name": "FILE_LOG",
      "value": "false"
    }
  ]
}
